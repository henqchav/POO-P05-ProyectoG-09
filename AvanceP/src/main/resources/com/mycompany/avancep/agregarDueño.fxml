<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="470.0" prefWidth="423.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.avancep.AgregarDueñoController">
    <children>
        <Label text="Agregar Dueño">
         <font>
            <Font name="System Bold" size="20.0" />
         </font></Label>
      <HBox prefHeight="20.0" prefWidth="306.0">
         <children>
            <Label text="Nombres:">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
               <HBox.margin>
                  <Insets right="52.0" />
               </HBox.margin>
            </Label>
            <TextArea fx:id="nombreDueno" prefHeight="37.0" prefWidth="258.0" />
         </children>
      </HBox>
      <HBox prefHeight="24.0" prefWidth="306.0">
         <children>
            <Label text="Apellidos:">
               <HBox.margin>
                  <Insets right="62.0" />
               </HBox.margin>
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
            <TextField prefHeight="25.0" prefWidth="247.0" />
         </children>
      </HBox>
      <HBox prefHeight="25.0" prefWidth="306.0">
         <children>
            <Label text="Dirección:">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
               <HBox.margin>
                  <Insets right="80.0" />
               </HBox.margin>
            </Label>
            <TextArea prefHeight="38.0" prefWidth="226.0" />
         </children>
      </HBox>
      <HBox prefHeight="26.0" prefWidth="306.0">
         <children>
            <Label text="Teléfono:">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
               <HBox.margin>
                  <Insets right="88.0" />
               </HBox.margin>
            </Label>
            <TextField prefHeight="25.0" prefWidth="225.0" />
         </children>
      </HBox>
      <HBox prefHeight="27.0" prefWidth="306.0">
         <children>
            <Label text="Ciudad:">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
               <HBox.margin>
                  <Insets right="74.0" />
               </HBox.margin>
            </Label>
            <ComboBox prefWidth="150.0" />
         </children>
      </HBox>
      <HBox prefHeight="21.0" prefWidth="306.0">
         <children>
            <Label prefHeight="21.0" prefWidth="55.0" text="Email:">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
               <HBox.margin>
                  <Insets right="74.0" />
               </HBox.margin>
            </Label>
            <TextField prefHeight="25.0" prefWidth="241.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="57.0" prefWidth="306.0">
         <children>
            <Button mnemonicParsing="false" text="Guardar">
               <HBox.margin>
                  <Insets right="30.0" />
               </HBox.margin>
            </Button>
              <Button fx:id="fx_return" onAction="#switchToAdministarDueño" text="Regresar">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
