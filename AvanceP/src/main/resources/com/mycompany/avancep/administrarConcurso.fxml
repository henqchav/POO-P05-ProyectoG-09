<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="333.0" prefWidth="558.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.avancep.AdministrarConcursoController">
   <children>
      <Label prefHeight="18.0" prefWidth="134.0" text="Administrar Concurso" />
      <TableView prefHeight="194.0" prefWidth="276.0">
        <columns>
          <TableColumn fx:id="codigoAdminConcurso" prefWidth="75.0" text="Codigo" />
          <TableColumn fx:id="nombreAdminConcurso" prefWidth="75.0" text="Nombre" />
            <TableColumn fx:id="fechasAdminConcurso" prefWidth="75.0" text="Fecha" />
            <TableColumn fx:id="ciudadAdminConcurso" prefWidth="75.0" text="Ciudad" />
            <TableColumn fx:id="opcionesAdminConcurso" prefWidth="218.0" text="Opciones" />
        </columns>
      </TableView>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="crearConcurso" mnemonicParsing="false" onAction="#switchToCrearConcurso" text="Crear Concurso">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="enviarConcurso" text="Enviar Invitaciones">
               <HBox.margin>
                  <Insets left="20.0" right="20.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="regresarConcurso" onAction="#switchToPrincipal" text="Regresar">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
