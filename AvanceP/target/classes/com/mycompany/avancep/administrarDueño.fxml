<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="333.0" prefWidth="679.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.avancep.AdministrarDueñoController">
   <children>
      <Label prefHeight="18.0" prefWidth="134.0" text="Administrar Dueño" />
      <TableView prefHeight="144.0" prefWidth="602.0">
        <columns>
          <TableColumn prefWidth="75.0" text="Codigo" />
          <TableColumn prefWidth="111.0" text="Nombres" />
            <TableColumn prefWidth="117.0" text="Apellidos" />
            <TableColumn prefWidth="107.0" text="Teléfono" />
            <TableColumn prefWidth="108.0" text="Ciudad" />
            <TableColumn prefWidth="120.0" text="Opciones" />
        </columns>
      </TableView>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="fx_registrar" mnemonicParsing="false" onAction="#switchtoAgregarDueño" text="Registrar">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="fx_regresar" mnemonicParsing="false" onAction="#switchToPrincipal" text="Regresar">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
